{
  "expo": {
    "name": "E-mory",
    "slug": "E-mory",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "myapp",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.emory.emory"
    },
    "android": {
      "package": "com.emory.emory",
      "permissions": []
    }
  }
}
